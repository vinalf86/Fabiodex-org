    <decisions>
        <name>Check_Days_to_Event</name>
        <label>Check Days to Event</label>
        <locationX>1722</locationX>
        <locationY>468</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>X30_Days</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Data_ricezione_saldo__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Days_to_event__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>30.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Check_Lead_or_Contact</targetReference>
            </connector>
            <label>30 Days</label>
        </rules>
        <rules>
            <name>X7_Days</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Days_to_event__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>7.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_User_Fabio</targetReference>
            </connector>
            <label>7 Days</label>
        </rules>
        <rules>
            <name>Today_plus_7</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Flow.CurrentDate</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>EventPlus7</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_User_Sabrina</targetReference>
            </connector>
            <label>Today plus 7</label>
        </rules>
    </decisions>
